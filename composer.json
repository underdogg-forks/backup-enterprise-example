{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=5.6.4",
    "arcanedev/laravel-settings": "0.1",
    "barryvdh/laravel-debugbar": "^2.4@dev",
    "barryvdh/laravel-ide-helper": "^2.4@dev",
    "baum/baum": "~1.1",
    "bosnadev/repositories": "0.*",
    "camroncade/timezone": "0.1",
    "creativeorange/gravatar": "~1.0",
    "doctrine/dbal": "^2.6@dev",
    "garethp/php-ews": "^0.9.6",
    "jenssegers/agent": "^2.4",
    "laracasts/flash": "~1.3",
    "laravel/framework": "5.3.*",
    "nwidart/laravel-modules": "^1.0",
    "libern/laravel-sql-logging": "^1.0@dev",
    "mexitek/phpcolors": "dev-master",
    "mgallegos/laravel-jqgrid": "^1.2",
    "predis/predis": "^1.1",
    "rtconner/laravel-tagging": "~2.2",
    "tylercd100/lern": "^3.8.1",
    "yaap/theme": "2.*",
    "zizaco/entrust": "dev-laravel-5"
  },
  "require-dev": {
    "filp/whoops": "^2.2",
    "fzaninotto/faker": "~1.4",
    "jeroen-g/laravel-packager": "dev-master",
    "mockery/mockery": "0.9.*",
    "phpspec/phpspec": "~2.1",
    "phpunit/phpunit": "~5.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  }
}
